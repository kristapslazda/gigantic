    $(this).on("click", function (e) {
      if (touchtime == 0) {
        // set first click
        touchtime = new Date().getTime();
        if ($(this).hasClass("sub-link")) {
          e.preventDefault();
        }
        $(this).toggleClass("open");
      } else {
        // compare first click to this click and see if they occurred within double click threshold
