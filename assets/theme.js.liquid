      arr.push($(this).data("for"));
    });

    for (i = 0; i < arr.length; i++) {
      if (arr[i] === elm.children("a").text().replace(/\s/g, "")) {
        elm.addClass("hasMega-menu");
        $(".mega-menu_section .mega-menu_for[data-for='"+ elm.children("a").text().replace(/\s/g, "") +"']").clone().insertAfter(elm.children("a"));
