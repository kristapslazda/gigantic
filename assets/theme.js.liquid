        .prepend('<option value="">Pick a ' + productOptions[i][i] + '</option>')
        .val('')
        .trigger('change');
    }
  }

  if ($('.product-single').length) {
    // get init slide if product has variants
    let selectedId = $('#ProductSelect [selected="selected"]').data('image-id');
    let initSlide = $('.thumb-' + selectedId).index();
    if (initSlide < 0) initSlide = 0;

    // product slider
    let productSlider = $(".product__image-slider").slick({
      slidesToShow: 1,
      slidesToScroll: 1,
      fade: true,
      initialSlide: initSlide,
      arrows: false,
      dots: false,
      speed: 80,
      onAfterChange: function(slider, index){
        slideThumbnailsStateChange(index);
        enableZoomForActiveSlide(index);
      },
      onInit: function (slider) {
        let currentSlide = slider.currentSlide;
        slideThumbnailsStateChange(currentSlide);
        let interval = setInterval(function () {
          if( $('.zoomContainer').length) {
            clearInterval(interval);
            enableZoomForActiveSlide(currentSlide);
          }
        }, 100);
      }
    });

    // thumbnails for product slider
    $('.nav-for-slider').on('mouseenter', function() {
      let _this = $(this);
      let index = _this.data('index'); 
      setTimeout(function() {
        slideThumbnailsStateChange(index);
        productSlider.slickGoTo(index);
      }, 100);
    });

    // zoom

    if (winWidth > 900) {

      // count zoom window width & height
      let sliderOffset = $('.product__image-slider').offset().left;
      let sliderWidth = $('.product__image-slider').outerWidth();
      let zoomWidnowWidth = winWidth - sliderWidth - sliderOffset;
      
      if (zoomWidnowWidth > 655) zoomWidnowWidth = 655;

      // init zoom for top slider
      $('.img-wrap img').elevateZoom({
        tintOpacity: 1,
        lensBorder: 0,
        lensOpacity: 1,
        borderSize: 0,
        zoomWindowWidth: zoomWidnowWidth - 30,
        zoomWindowHeight: sliderWidth - 20,
        zoomWindowOffetx: 20,
        zoomWindowOffety: 1
      });
    }
  }
  
  // init other sliders besides "recomendation"
  if (winWidth > 770) {
    let corusel = $('.product-list-slider:not(.recomendation)');
    corusel.each(function () {
      let leftArrow = $(this).find('.col-slider-prev');
      let rightArrow =$(this).find('.col-slider-next');
      let slider = $(this).find('.multi-slides');
      slider.slick({
        prevArrow: leftArrow,
        nextArrow: rightArrow,
        slidesToShow: 4,
        slidesToScroll: 1,
        autoplay: false,
        infinite: false,
        swipeToSlide: true,
        responsive: [
            {
                breakpoint: 1440,
                settings: {
                    slidesToShow: 4
                }
            }
        ]
      });
    });
  } else {
    let corusels = document.querySelectorAll('.product-list-slider:not(.recomendation) .multi-slides');
    corusels.forEach(function(item) {
      new Glider(item, 
        {
          slidesToShow: 3.5,
          slidesToScroll: 3.5,
          duration: 1,
          draggable: true,
            responsive: [
            {
                breakpoint: 490,
                settings: {
                    slidesToShow: 2.5,
                    slidesToScroll: 2.5,
                }
            },
            {
                breakpoint: 320,
                settings: {
                    slidesToShow: 1.5,
                    slidesToScroll: 1.5,
                }
            }
          ]
        }
      );
    });
  }

  // mobile tabs 
  let mq = window.matchMedia( "(max-width: 1025px)" );

  let colapsedTextHolder = $('.tabs__content .colapsed-text');

