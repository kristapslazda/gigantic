      $(this).parent().find(".tabs__content-item--active").removeClass("tabs__content-item--active");
      $(this).find(".tabs__content-item").addClass("tabs__content-item--active");
    });
    
    colapsedTextHolder = $('.tabs__nav .colapsed-text');
  }

  console.log(colapsedTextHolder);
