// functions 

function slideThumbnailsStateChange (index) {
  $('.nav-for-slider.selected').removeClass('selected');
  $('.nav-for-slider[data-index="' + index + '"]').addClass('selected');
}

 // set class "visible" to active slide's zoom wrappe  
 function enableZoomForActiveSlide (slideId) {
  let imageName = $('.img-wrap[index="' + slideId + '"]').data('image-name');
  let zoomWrapper = $('.zoomContainer');
    
  zoomWrapper.removeClass('visible');
  
  zoomWrapper.each(function() {
    if ($(this).find('.zoomWindowContainer > div').attr('style').indexOf(imageName) != -1) {
      $(this).addClass('visible');
    }
  });
}

// check if Explorer
function msieversion() {
  var ua = window.navigator.userAgent;
  var msie = ua.indexOf("MSIE ");

  if (msie > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\./)) {
      return 1;
  }
  return false;
}

// UPsell modal functions
function updateModalStock(chosen_variant, product_id) {
  let variant_qty = chosen_variant.data('inventory-quantity'),
      qty_status_el = $('.modal-stock-status-wrapper[data-variant-for-id="' + product_id + '"]'),
      html_in_stock = '';
      
  console.log('updated stock div with variant ' + chosen_variant.val() + ' and product id ' + product_id + ' and it has ' + variant_qty + ' items');
  
  qty_status_el.data('variant-for-id', chosen_variant.val());
  
  if( variant_qty >= 10 ){
    html_in_stock = '<label class="in-stock">{{ settings.stock_label_above_10 }}</label> <p class="stock-status">{{ settings.delivery_time_above_10 }}</p>';
  } else {
    html_in_stock = '<label class="out_of_stock">{{ settings.stock_label_below_0 }}</label> <p class="stock-status">{{ settings.delivery_time_below_0 }}</p>';
  }
  
  qty_status_el.html(html_in_stock);
}

// visual price update 
// take product variant as argumen
function visualPriceUpdate (variant) {
  let price = variant.price;
  let compare_at_price = variant.compare_at_price;
  let variantPrice = theme.Currency.formatMoney(price, theme.strings.moneyFormat);
  let quantitySalesWrapper = $('.product-bundles_wrapper');

  if(quantitySalesWrapper.length) {
    let quantitySalesItmes = quantitySalesWrapper.find('li');
    quantitySalesItmes.each(function() {
      let salesPercent = $(this).data('percent');
      let currentSalesPrice = price - (price * salesPercent / 100);
      $(this).find('.price').text(theme.Currency.formatMoney(currentSalesPrice, theme.strings.moneyFormat));
    });
  }

  if(compare_at_price) {
    let compareAtPrice = theme.Currency.formatMoney(compare_at_price, theme.strings.moneyFormat);
    $('.price__regular .price-item--regular').text(compareAtPrice);
    $('.price__sale .price-item--sale').text(variantPrice);
  } else {
    $('.price__regular .price-item--regular').text(variantPrice);
  }
};

// custom menu first level links hover event
function hoveMenuItems (link) {
  if($(window).width() > 1024) {
    let _this = link;
    let wrapperId = $(_this).data('link-id');
    let advancedBlock = $('.advanced-block-single[data-wrapper-id="' + wrapperId + '"]'); 

    let collectionTitle = $(_this).data('collection-name');

    $('.block-vissible').removeClass('block-vissible');
    advancedBlock.addClass('block-vissible');
    advancedBlock.find('.featured-products .visible').removeClass('visible');
    advancedBlock.find('.first-level-products').addClass('visible');
    advancedBlock.find('h3 .category-name').text(collectionTitle);
  } else {
    return false;
  }
}

// custom menu second level links hover event
function hoveSecondMenuItems (link) {
  if($(window).width() > 1024) {
    let _this = link;
    let collName = $(_this).data('collection-name');
    let curentRelatedProductsWrapper = $(_this).closest('.sub-menu-block').siblings('.featured-products');
    let collectionLink = $(_this).attr('href');
    curentRelatedProductsWrapper.find('h3 .category-name').text(collName);
    curentRelatedProductsWrapper.find('.visible').removeClass('visible');
    curentRelatedProductsWrapper.find('[data-collection="' + collectionLink + '"]').addClass('visible');
  } else {
    return false;
  }
}

// read more button event
function readMoreClick (button, textHolder) {
  let _this = button;
  if(_this.hasClass('open')) {
    _this.removeClass('open');
    textHolder.css('height', textHolder.data('height-close') + 'px');
    _this.text(_this.data('read-more'));
  } else {
    _this.addClass('open');
    textHolder.css('height', textHolder.data('height-total') + 'px');
    _this.text(_this.data('read-less'));
  }
}

// read more return object
// child which has to be cutted
// how many lines should be disaplayed
// line height of target element
function countChildrenTotalHeight (wrapper, lineHeight) {
  let children = wrapper.children();
  let totalLines = 0;
  let totalMargin = 0;
  
  if (children.length) {
    for (let i = 0; i < children.length; i++) {
      let childHeight = $(children[i]).outerHeight();
      let childMarginBottom = $(children[i]).css('margin-bottom').slice(0, -2);
      let currentLines = countLines(childHeight, lineHeight);
      totalLines = totalLines + currentLines;
      if(totalLines >= 5) {
        return {
          targetObject: $(children[i]),
          leftedLine: 5 - (totalLines - currentLines),
          margin: +totalMargin,
          lineHeight: +$(children[i]).css('line-height').slice(0, -2)
        };
      } else {
        totalMargin = totalMargin + +childMarginBottom;
      }
    }
  } else {
    return false;
  }
}

// return number of lines of element
function countLines (itemHeight, lineHeight) {
  let numberOfLines = Math.floor(itemHeight / lineHeight);
  return numberOfLines
}