$(document).on('submit', '.js-upsell-cart-section [action="/cart/add"]', function(evt) {
  evt.preventDefault();
  console.log('added');
  // Add class to be styled if desired
  $(this).find('[type="submit"]').removeClass('is-added').addClass('is-adding');
  var formSel = evt.target;

  (function() {
    var params = {
      type: 'POST',
      url: '/cart/add.js',
      data: jQuery(formSel).serialize(),
      dataType: 'json',
      success: function(line_item) {
        window.dispatchEvent(new CustomEvent('itemAdded', { detail: line_item }));
        setTimeout(function () {
          jQuery.getJSON("/cart.js", function(cart) {
            $(".cart--count").text(cart.item_count);
            var i,
                cartItemArr = [],
                upsellArr = [];
                $(".js-upsell-cart-section .upsell-prod").each(function() {
                  upsellArr.push($(this).data("prod-id"));
                });
            for (i = 0; i < cart.items.length; i++) {
              cartItemArr.push(cart.items[i].product_id);

              if (upsellArr.indexOf(cartItemArr[i]) != -1) {
                $(".js-upsell-cart-section [data-prod-id='"+ cartItemArr[i] +"']").hide();
              }
            }
          });
          ajaxCart.load();
                    
          let modalBtn = $(formSel).find('.js-upsell-modal-submit'),
              modalBtnLoad = modalBtn.find('.is-add-text'),
              modalBtnText = modalBtn.find('.btn__text'),
              modal = $('.upsell-modal');

          modalBtnLoad.hide();
          modalBtnText.show();    
          // modalBtn.attr("disabled", true);
          // modalBtn.attr('data-is-in-cart', 'true');
          modal.removeClass('modal-open');
          
        }, 250)
      },
      error: function(XMLHttpRequest, textStatus) {
        var data = eval('(' + XMLHttpRequest.responseText + ')');
        console.log(data.message + '(' + data.status + '): ' + data.description);
      }
    };
    jQuery.ajax(params);
  })();
});
    
    
