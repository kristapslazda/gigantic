          $parents = $nav.find(selectors.hasDropdown),
          $childLinks = $nav.find(selectors.childLink),
          $topLevel = $parents.find(selectors.nextLink),
          $dropdowns = $nav.find(selectors.dropdown),
          $subMenuLinks = $dropdowns.find(selectors.link);

