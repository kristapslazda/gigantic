/* GRAFIKR CUSTOM CSS */  
/* Change image slider on product */
.template-product {
  .product-single .product__image-wrapper .img-wrap:nth-child(n+7) {display:block!important;}
  .prod-prev, .prod-next {
    position: absolute;
    left: 1rem;
    top: 50%;
    width: 40px;
    height: 40px;
    transform: translateY(-50%);
    cursor:pointer;
  }

  .prod-next {
    left: auto;
    right: 52px;
  }

  .product-form__sticky__submit .btn {padding: 20px;}

  .main-image-slider,
  .product__image-slider {
    width: 100%;
    .slick-dots {
      padding: 0;
      list-style: none;
      display: flex;
      position: absolute;
      bottom: 1rem;
      left: 0;
      right: 0;
      justify-content: center;
      z-index: 100;
      li {
        button {
          border-radius: 50%;
          height: 15px;
          width: 15px;
          background-color: #0766a1;
          margin-right: .5rem;
          cursor: pointer;
          border: none;
          color: #0766a1;
          opacity: .25;
          transition: .3s all ease;
          font-size: 0px;
          &:hover {
            opacity: .5;
          }
        }
        &.slick-active {
          button {
            opacity: 1;
          }
        }
      }
    }
  }

  .main-image-slider {
    .slick-dots  {
      position: static;
      li button {
        width: 9px;
        height: 9px;
        padding: 0;
      }
    }
  }

  .product__image-wrapper {
    position: relative;
  }
}